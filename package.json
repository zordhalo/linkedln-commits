{
  "name": "linkedin-commits",
  "version": "1.0.0",
  "description": "Track LinkedIn activity similar to how GitHub displays commits",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "example:oauth": "node src/examples/oauth-usage.js",
    "db:init": "node database/scripts/init.js",
    "db:init:fresh": "node database/scripts/init.js --drop",
    "db:seed": "node database/scripts/seed.js",
    "db:setup": "npm run db:init && npm run db:seed",
    "db:validate": "node database/scripts/validate.js",
    "db:examples": "node database/examples.js"
  },
  "keywords": [
    "linkedin",
    "activity",
    "tracking",
    "commits"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "crypto": "^1.0.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "mongoose": "^7.8.4"
  },
  "devDependencies": {
    "dotenv": "^16.3.1"
  }
}
